{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-pdf.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "wkhtmltopdf",
          "node_modules/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/generate-pdf",
      "dest": "api/generate-pdf.js"
    }
  ]
}
